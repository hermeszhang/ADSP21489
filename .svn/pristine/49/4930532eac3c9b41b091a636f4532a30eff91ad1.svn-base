<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/colorBackGround"
    android:orientation="vertical">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="测试接收回复"
        android:background="@color/colorPrimary"
        android:textColor="@color/colorWhite"
        android:gravity="center"
        android:padding="@dimen/padding_button_text_large"
        />

    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="vertical">

            <Button
                android:id="@+id/ack_btn_connect_ok"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="1、连接正常"/>

            <Button
                android:id="@+id/ack_btn_hibernate_ok"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="2A、休眠成功"/>
            <Button
                android:id="@+id/ack_btn_hibernate_error"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="2B、休眠失败"/>
            <Button
                android:id="@+id/ack_btn_adsp_start_ok"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="3A、接收器启动成功"/>
            <Button
                android:id="@+id/ack_btn_adsp_start_error"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="3B、接收器启动失败"/>
            <Button
                android:id="@+id/ack_btn_adsp_stop_ok"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="3C、接收器关闭成功"/>
            <Button
                android:id="@+id/ack_btn_adsp_stop_error"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="3D、接收器关闭失败"/>
            <Button
                android:id="@+id/ack_btn_software_version"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="4、软件版本"/>
            <Button
                android:id="@+id/ack_btn_product_id"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="5、接收器产品ID"/>
            <Button
                android:id="@+id/ack_btn_snr_rate"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="6、信噪比"/>
            <Button
                android:id="@+id/ack_btn_snr_status"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="7、接收状态"/>
            <Button
                android:id="@+id/ack_btn_time_offset"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="8、时偏"/>
            <Button
                android:id="@+id/ack_btn_freq_offset"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="9、频偏"/>
            <Button
                android:id="@+id/ack_btn_tuner_status"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="10、Tuner状态"/>
            <Button
                android:id="@+id/ack_btn_task_list"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="11、任务清单"/>
            <Button
                android:id="@+id/ack_btn_date"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="12、当前日期"/>
            <Button
                android:id="@+id/ack_btn_ckfo_enable_ok"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="13A、设置校验失败输出：成功"/>
            <Button
                android:id="@+id/ack_btn_ckfo_enable_error"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="13B、设置校验失败输出：失败"/>
            <Button
                android:id="@+id/ack_btn_ckfo_disable_ok"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="13C、设置校验失败不输出：成功"/>
            <Button
                android:id="@+id/ack_btn_ckfo_disable_error"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="13C、设置校验失败不输出：失败"/>
            <Button
                android:id="@+id/ack_btn_set_baud_rate_ok"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="14A、设置波特率成功"/>
            <Button
                android:id="@+id/ack_btn_set_baud_rate_error"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="14B、设置波特率失败"/>
            <Button
                android:id="@+id/ack_btn_set_freq_ok"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="15A、设置频点成功"/>
            <Button
                android:id="@+id/ack_btn_set_freq_error"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="15B、设置频点失败"/>
            <Button
                android:id="@+id/ack_btn_set_tunes_ok"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="16A、设置接收模式成功"/>
            <Button
                android:id="@+id/ack_btn_set_tunes_error"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="16B、设置接收模式失败"/>
            <Button
                android:id="@+id/ack_btn_set_1pps_open_ok"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="17A、设置1pps开启：成功"/>
            <Button
                android:id="@+id/ack_btn_set_1pps_open_error"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="17A、设置1pps开启：失败"/>
            <Button
                android:id="@+id/ack_btn_set_1pps_close_ok"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="17C、设置1pps关闭：成功"/>
            <Button
                android:id="@+id/ack_btn_set_1pps_close_error"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="17C、设置1pps关闭：失败"/>
            <Button
                android:id="@+id/ack_btn_prepare_upgrade_ok"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="18A、升级准备：成功"/>
            <Button
                android:id="@+id/ack_btn_prepare_upgrade_error"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:text="18A、升级准备：失败"/>

        </LinearLayout>
    </ScrollView>


</LinearLayout>
